{
    "name": "faqforge",
    "version": "1.0.0",
    "description": "Contextual FAQ Generator for Websites using MERN Stack and OpenAI",
    "main": "server.js",
    "scripts": {
        "server": "nodemon backend/server.js",
        "client": "npm start --prefix frontend",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "start": "node backend/server.js",
        "build": "npm install --prefix frontend && npm run build --prefix frontend"
    },
    "keywords": [
        "faq",
        "openai",
        "mern",
        "crawler"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "axios": "^1.6.2",
        "cheerio": "^1.0.0-rc.12",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "mongoose": "^8.0.3",
        "openai": "^4.20.1"
    },
    "devDependencies": {
        "concurrently": "^8.2.2",
        "nodemon": "^3.0.2"
    }
}